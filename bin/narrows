#!/usr/bin/env python

import sys
if sys.version_info[:2] < (3, 6):
    v_info = sys.version_info[:3]
    sys.exit('Narrows requires Python 3.6 or higher. '
             'This is Python %d.%d.%d.' % v_info)

import os
MY_DIR = os.path.dirname(__file__)
sys.path.append(f'{MY_DIR}/..')

import narrows
sys.exit(narrows.main())
